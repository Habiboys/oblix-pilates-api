# Shared Hosting Environment Configuration
NODE_ENV=shared
PORT=3000

# Database Configuration
DB_HOST=localhost
DB_USER=your_db_user
DB_PASS=your_db_password
DB_NAME=your_db_name
DB_PORT=3306

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=24h

# Email Configuration (optional - disable if not needed)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your_email@gmail.com
EMAIL_PASS=your_email_password

# Midtrans Configuration (optional - disable if not needed)
MIDTRANS_SERVER_KEY=your_midtrans_server_key
MIDTRANS_CLIENT_KEY=your_midtrans_client_key
MIDTRANS_IS_PRODUCTION=false

# Twilio Configuration (optional - disable if not needed)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Redis Configuration (optional - disable if not needed)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Memory Optimization
NODE_OPTIONS=--max-old-space-size=512 