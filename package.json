{
  "name": "be-starter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed",
    "db:seed:undo": "sequelize-cli db:seed:undo",
    "docs:build": "node docs/build-docs.js",
    "docs:watch": "nodemon --watch docs --ext yaml --exec 'npm run docs:build'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t oblix-pilates-api .",
    "docker:run": "docker run -p 3000:8080 oblix-pilates-api",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "deploy": "chmod +x deploy.sh && ./deploy.sh",
    "deploy:secure": "chmod +x deploy-secure.sh && ./deploy-secure.sh",
    "setup:secrets": "chmod +x setup-secrets.sh && ./setup-secrets.sh",
    "cleanup:secrets": "chmod +x cleanup-secrets.sh && ./cleanup-secrets.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.0",
    "express": "^5.1.0",
    "joi": "^17.13.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1",
    "nodemailer": "^7.0.5",
    "nodemon": "^3.1.10",
    "redis": "^5.5.6",
    "sequelize": "^6.37.7",
    "sharp": "^0.34.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0",
    "midtrans-client": "^1.3.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^30.0.4",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.3"
  }
}
